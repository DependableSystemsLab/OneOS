{
  "name": "oneos",
  "version": "20.1.0",
  "description": "Overlay Network Operating System",
  "main": "lib/oneos.js",
  "files": [
    "bin/fixtures/*",
    "bin/scripts/*",
    "lib/*"
  ],
  "scripts": {
    "test": "mocha test/run-all.js --exit",
    "test-script": "node test/run-script.js",
    "lint": "eslint lib --fix",
    "build": "node node_modules/.bin/webpack --config webpack.prod.js --progress",
